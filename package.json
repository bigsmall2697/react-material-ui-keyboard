{
  "name": "react-material-ui-keyboard",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typings": "typings install",
    "start": "webpack && mv ./demo/index.html ./ && node server.js",
    "prepublish": "npm run prepublish:rm && npm run prepublish:tsc && npm run prepublish:sed && npm run prepublish:mv && npm run prepublish:node",
    "prepublish:rm": "rm -Rf ./d*",
    "prepublish:tsc": "tsc -d -m umd  --moduleResolution node -t es5 --jsx react --noImplicitAny --outDir ./dist ./src/index.tsx",
    "prepublish:sed": "sed -i '/\\/\\/\\/ <reference path=\".\\/..\\/typings\\/index.d.ts\" \\/>/d' ./dist/index.js",
    "prepublish:mv": "mkdir ./dts && mv ./dist/*.d.ts ./dts",
    "prepublish:node": "node ./prepare_dts.js && rm -R ./dts"
  },
  "keywords": [
    "react",
    "material-ui",
    "keybaord",
    "on-screen-keyaborad",
    "virtual-keyaboard"
  ],
  "author": "Ivo Stratev",
  "license": "MIT",
  "dependencies": {
    "material-ui": "^0.15.2",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "react-tap-event-plugin": "^1.0.0"
  },
  "devDependencies": {
    "express": "^4.14.0",
    "html-webpack-plugin": "^2.22.0",
    "react-hot-loader": "^1.3.0",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
