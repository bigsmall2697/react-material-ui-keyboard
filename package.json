{
  "name": "react-material-ui-keyboard",
  "version": "6.0.6",
  "description": "Virtual keyboard for TextFeild when needed.",
  "main": "./index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typings": "typings install",
    "start": "webpack && mv ./demo/index.html ./ && node ./server.js",
    "prepublish": "npm run prepublish:rm && npm run prepublish:tsc && npm run prepublish:sed && npm run prepublish:dts",
    "prepublish:rm": "rm -Rf ./d*",
    "prepublish:tsc": "tsc -d -m commonjs  --moduleResolution node -t es5 --jsx react --noImplicitAny --experimentalDecorators --outDir . ./src/index.tsx",
    "prepublish:sed": "npm run prepublish:sed:typings",
    "prepublish:sed:typings": "sed -i '/\\/\\/\\/ <reference path=\".\\/..\\/typings\\/index.d.ts\" \\/>/d' ./index.js",
    "prepublish:dts": "echo  \"import Keyboard from './Keyboard';\nexport * from './Keyboard';\nexport default Keyboard;\" > ./index.d.ts",
    "postpublish": "rm ./index* ./layouts* ./Keyboard*"
  },
  "keywords": [
    "react",
    "material-ui",
    "keyboard",
    "on-screen-keyboard",
    "virtual-keyboard"
  ],
  "author": "Ivo Stratev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NoHomey/react-material-ui-keyboard.git"
  },
  "peerDependencies": {
    "material-ui": "^0.15.4",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-tap-event-plugin": "^1.0.0"
  },
  "devDependencies": {
    "material-ui": "^0.15.4",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-tap-event-plugin": "^1.0.0",
    "express": "^4.14.0",
    "html-webpack-plugin": "^2.22.0",
    "react-hot-loader": "^1.3.0",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "bind-decorator": "^1.0.3",
    "deep-equal": "^1.0.1",
    "object-assign": "^4.1.0"
  }
}
